{
  "name": "pageRotate",
  "description": "A chrome extension that rotates the current page",
  "version": "0.0.1",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icon-34.png"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": [],
      "js": ["contentScript.bundle.js"]
    }
  ],
  "permissions": ["activeTab"]
}
